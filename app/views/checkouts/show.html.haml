= render :partial => "menu/category"
#page.grid_9
  %h1 Checkout

  %table#checkout
    %tr
      %th.first_column Quantity
      %th Title
    - @cart_lines.each do |cart_line|
      %tr[cart_line]
        %td=cart_line.quantity
        %td=cart_line.wine.title

  =link_to 'edit', cart_path
  |
  =link_to 'continue', details_cart_checkout_path
